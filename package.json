{
  "name": "babel-plugin-qunit-lazy-imports",
  "version": "1.1.1",
  "description": "Babel plugin that transforms imports in QUnit test files to lazy imports in beforeEach hooks",
  "keywords": [
    "babel",
    "plugin",
    "qunit",
    "lazy",
    "imports",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:NullVoxPopuli/babel-plugin-qunit-lazy-imports.git"
  },
  "license": "MIT",
  "author": [
    "NullVoxPopuli",
    "mansona"
  ],
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "format": "prettier . --write",
    "lint:prettier": "prettier . --check",
    "lint:package": "publint",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "test": "pnpm run test:unit && pnpm run test:integration",
    "test:integration": "cd test-app && pnpm test",
    "test:unit": "vitest",
    "test:watch": "node --test --watch tests/*.test.js"
  },
  "dependencies": {
    "@babel/template": "^7.27.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/generator": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "concurrently": "^9.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "^2.0.6",
    "publint": "^0.3.12",
    "release-plan": "^0.16.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": "^20.11 || >= 22"
  }
}
